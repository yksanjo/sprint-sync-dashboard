{
  "name": "sprint-sync-dashboard",
  "version": "1.0.0",
  "description": "Developer productivity tool that aggregates sprint health metrics across GitHub, Jira/Linear, and Slack",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:web",
    "build:web": "cd web && npm install && npm run build",
    "start": "node dist/server/index.js",
    "start:worker": "node dist/index.js",
    "dev": "tsx src/server/index.ts",
    "dev:worker": "tsx src/index.ts",
    "test": "vitest",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "sprint",
    "github",
    "jira",
    "linear",
    "slack",
    "productivity",
    "metrics"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/graphql": "^7.0.0",
    "@prisma/client": "^5.7.0",
    "@slack/bolt": "^3.17.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.0.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "stripe": "^14.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}


